<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="Staff" table="data">

        <id name="staff_id" column="staff_id" type="int"><generator class="identity"/></id>
        <property name="ime" type="string" column="ime"/>
        <property name="broj_godina" column="broj_godina" type="int"/>
        <property name="adresa" type="string" column="adresa"/>
        <property name="visina_dohotka" column="visina_dohotka" type="int"/>

    </class>
</hibernate-mapping>